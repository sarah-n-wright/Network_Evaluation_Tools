### DATA ARGUMENTS ###
## REQUIRED ##

# name to be given to the network
name=wan
# full filepath of the input data
input_datafile=/cellar/users/snwright/Data/Network_Analysis/Network_Data_Raw/Primary_Datasets/Wan_2015_PPIs.txt
# full directory for saving processed data
outpath=/cellar/users/snwright/Data/Network_Analysis/Processed_Data/v2_2022/

## OPTIONAL ##

# which separator is used in the input data? Default tab separated
separator="tab"
# Which row should be treated as a header? "0" for first row. "None" if there is no header
header="0"

### PROCESSING ARGUMENTS ###
## REQUIRED ##

# column name for interactor A, or integer index
nodeA_col=ENSG1
# column name for interactor B, or integer index
nodeB_col=ENSG2
# identifiers used in raw data, one of Symbol, Uniprot, Entrez, Ensembl.  For multiple space separate and enclose in quotes
input_id_type=Ensembl
# identifiers to be used in processed data. One of Symbol, Uniport, Entrez, Ensembl
target_id_type=Entrez


## OPTIONAL ##

# column name for score or integer index. If no score column, enter "None". If scores are pvalues add "[p]" to the end to generate -log10(pvalues)
score_col="Cofrac_PPI_score"
# column name for species or integer index. If no species column, enter "None". If multiple columns separate with ";"
species_col="None"
# values within the species column to be considered. If none, enter "None"
species_values="None"
# if node ids are preceded by a prefix, indicate here. If multiple ids included also add the separator.
# Format is "prefix[sep],prefix[sep]" E.g. for DIP-xxxx|uniprotkb:xxxxx use "uniprotkb+|" and or "DIP-+|"
node_prefix_separator="None"
